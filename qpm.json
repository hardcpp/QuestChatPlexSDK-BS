{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "ChatPlexSDK-BS",
    "id": "chatplex-sdk-bs",
    "version": "1.0.0-v6.2.0",
    "url": "https://github.com/hardcpp/QuestChatPlexSDK-BS",
    "additionalData": {
      "overrideSoName": "libchatplex-sdk-bs.so",
      "cmake": true
    }
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^3.14.0",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "codegen",
      "versionRange": "^0.33.0",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.15.24",
      "additionalData": {}
    },
    {
      "id": "modloader",
      "versionRange": "^1.2.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.3.1",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "libil2cpp",
      "versionRange": "^0.2.3",
      "additionalData": {}
    },
    {
      "id": "songloader",
      "versionRange": "^0.10.17",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "conditional-dependencies",
      "versionRange": "*",
      "additionalData": {
        "private": true
      }
    }
  ],
  "workspace": {
    "scripts": {
      "copy": [
        "pwsh ./copy.ps1"
      ],
      "build": [
        "pwsh ./build.ps1"
      ],
      "log": [
        "pwsh ./start-logging.ps1"
      ],
      "clean": [
        "pwsh ./build.ps1 -clean"
      ],
      "qmod": [
        "pwsh ./build.ps1 -clean",
        "qpm qmod build",
        "pwsh ./createqmod.ps1 -clean"
      ]
    }
  }
}